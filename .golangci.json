{
	"linters": {
		"disable-all": true,
		"enable": [
			"govet",
			"revive",
			"goimports",
			"misspell",
			"ineffassign",
			"gofmt"
		]
	},
	"run": {
		"skip-files": [
			"/zz_generated_"
		],
		"deadline": "5m"
	},
    "issues": {
        "exclude-rules": [
            {
                "linters": "revive",
                "text": "should have comment or be unexported"
            }
        ]
    }
}
